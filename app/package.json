{
  "name": "backend-service",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env PROJECT_NAME=project_name STAGE=local nest start --watch | pino-pretty -m message --levelKey logLevel -a timestamp",
    "start:debug": "cross-env PROJECT_NAME=project_name STAGE=local nest start --debug --watch | pino-pretty -m message --levelKey logLevel -a timestamp",
    "start:prod": "node dist/main",
    "prelint": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "depcheck": "depcheck --skip-missing --ignores=\"@types/jest\""
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/cli": "10.1.17",
    "@nestjs/common": "10.2.5",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.5",
    "@nestjs/platform-express": "10.2.5",
    "@nestjs/swagger": "7.1.11",
    "@nestjs/terminus": "10.0.1",
    "@willsoto/nestjs-prometheus": "6.0.1",
    "axios": "1.7.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "jwks-rsa": "3.1.0",
    "nestjs-pino": "3.1.1",
    "pino-http": "8.2.1",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.5",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.12",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "cross-env": "7.0.3",
    "depcheck": "^1.4.6",
    "eslint": "8.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest": "27.4.5",
    "pino-pretty": "9.1.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.21",
    "supertest": "6.1.6",
    "swagger-ui-express": "4.4.0",
    "ts-jest": "27.1.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@1.22.19"
}
